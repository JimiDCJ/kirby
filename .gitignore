body{
  background:#1D1E22;
  display:flex;
  flex-direction:column;
  justify-content:center;
  align-items:center;
  height:100vh;
}
.frame{
  z-index:-1;
  position:relative;
  width:400px;
  height:400px;
  background:yellow;
  border-radius:15px;
  justify-contents:center;
}
.bodyy{
  background:#F3A5AA;
  position:absolute;
  width:250px;
  height:250px;
  top:18%;
  left:20%;
  border-radius:50%;
}
.left-arm{
  position:absolute;
  background:#F3A5AA;
clip-path: ellipse(30% 61% at 48% 98%);
  height:150px;
  width:150px;
  transform:rotate(-45deg);
  left:-30%;
  top:-20%;
}
.right-arm{
  position:absolute;
  background:#F3A5AA;
clip-path: ellipse(30% 61% at 48% 98%);
  height:150px;
  width:150px;
  transform:rotate(130deg);
  right:-30%;
  top:60%;
}
.left-foot{
  position:absolute;
  background:#E0005B;
  width:200px;
  height:150px;
clip-path: ellipse(24% 45% at 51% 50%);
  top:55%;
  left:-25%;
  transform:rotate(-30deg);
}
.right-foot{
  position:absolute;
   background:#E0005B;
  width:200px;
  height:150px;
  top:65%;
  right:-12%;
  transform:rotate(150deg);
  z-index:-1;
clip-path: ellipse(22% 56% at 57% 72%);
}
.left-eye{
  transform:rotate(8deg);
  position:absolute;
  height:65px;
  width:56px;
  background:black;
  left:28%;
  top:20%;
clip-path: ellipse(21% 40% at 50% 50%);

}
.inner-one{
  background:#0076C0;
  position:absolute;
  height:60px;
  top:3%;
  left:2%;
  width:53px;
  clip-path: ellipse(20% 40% at 50% 50%);
}

.inner-two{
  background:black;
  position:absolute;
  height:40px;
  
  width:53px;
  clip-path: ellipse(18% 40% at 50% 50%);
}
.inner-three{
  background:white;
  position:absolute;
  height:30px;
  top:7%;
  width:53px;
  clip-path: ellipse(12% 36% at 50% 50%);
}
.right-eye{
  position:absolute;
left:52%;
  transform:rotate(-10deg);
  top:35%;
  width: 75px; 
  height: 100px;  
  border: solid 4px #000;
  border-color: #000 transparent transparent transparent;
  border-radius: 87%/95px 48px 0 0; 
}
.left-dimple{
  width:41px;
  height:41px;
transform:rotate(9deg);
  top:40%;
  left:13%;
clip-path: ellipse(49% 26% at 50% 50%);
background:#EB6896;
  position:absolute;
}
.right-dimple{
    width:41px;
  height:41px;
transform:rotate(4deg);
  top:47%;
  right:25%;
clip-path: ellipse(49% 26% at 50% 50%);
background:#EB6896;
  position:absolute;
  
}
.smile {
  height:45px;
  width: 23px;
  border-radius: 50px 0 0 50px;
  border-top: 3.5px solid #000;
  border-left: 3.5px solid #000;
  border-bottom: 3.5px solid #000;
  transform: translate(100px) rotate(-72deg);
  top:52%;
  left:-2%;
  position:absolute;
}
